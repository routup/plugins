{
    "name": "@routup/static",
    "version": "0.2.1",
    "description": "Routup plugin for serving static files.",
    "main": "./dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rimraf ./dist && tsc -p tsconfig.build.json",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage",
        "lint": "eslint --ext .js,.ts ./src",
        "lint:fix": "npm run lint -- --fix",
        "prepublishOnly": "npm run build"
    },
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "keywords": [
        "file",
        "files",
        "serve",
        "static",
        "api",
        "rest",
        "http",
        "router",
        "api-router",
        "route",
        "routing"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tada5hi/routup.git",
        "directory": "packages/static"
    },
    "bugs": {
        "url": "https://github.com/Tada5hi/routup/issues"
    },
    "homepage": "https://github.com/Tada5hi/routup#readme",
    "peerDependencies": {
        "@routup/core": "^0.x"
    },
    "devDependencies": {
        "@routup/core": "^0.3.0",
        "@types/supertest": "^2.0.12",
        "routup": "^0.9.1",
        "supertest": "^6.3.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
