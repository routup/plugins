{
    "name": "@routup/decorators",
    "version": "0.3.0",
    "description": "Decorators plugin for routup.",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "assets/",
        "dist/"
    ],
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "npx swc ./src -d dist --config-file ../../.swcrc.json",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage",
        "lint": "eslint --ext .js,.ts ./src",
        "lint:fix": "npm run lint -- --fix",
        "prepublishOnly": "npm run build"
    },
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "keywords": [
        "decorators",
        "annotations",
        "api",
        "rest",
        "http",
        "router",
        "api-router",
        "route",
        "routing"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tada5hi/routup.git",
        "directory": "packages/decorators"
    },
    "bugs": {
        "url": "https://github.com/Tada5hi/routup/issues"
    },
    "homepage": "https://github.com/Tada5hi/routup#readme",
    "peerDependencies": {
        "routup": ">=0.12.0 <1.0.0"
    },
    "devDependencies": {
        "@routup/body": "^0.5.0",
        "@routup/cookie": "^0.5.0",
        "@routup/query": "^0.5.0",
        "@types/supertest": "^2.0.12",
        "routup": "^0.11.0",
        "supertest": "^6.3.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
